{
  "name": "icrush-test",
  "version": "0.2.0",
  "description": "📚 iCrush测试用例",
  "scripts": {
    "start": "npm install && npm run dev",
    "dev": "node_modules/.bin/webpack-dev-server --config webpack.dev.js",
    "build": "node_modules/.bin/webpack --config webpack.prod.js",
    "debug": "npm run debug_echo && node --inspect-brk ./node_modules/webpack/bin/webpack.js --config webpack.prod.js",
    "debug_echo": "echo 'chrome://inspect/#devices'"
  },
  "keywords": [
    "iCrush",
    "test"
  ],
  "author": "心叶",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.1",
    "file-loader": "^4.0.0",
    "postcss": "^7.0.14",
    "postcss-loader": "^3.0.0",
    "style-loader": "^0.23.1",
    "url-loader": "^2.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@yelloxing/normalize.css": "^0.2.0"
  }
}
